<!DOCTYPE html>
<html>
<head>
  <title>{{title}}</title>
  <link rel="stylesheet" href="./test-css/icons.css">
</head>
<body>
  <header class="RSG-header">
    <h1>{{title}}</h1>
  </header>
  <navigation class="RSG-sidebar">
    <ul>
      {{#blocks}}
        <li><a href="#{{title}}">{{title}}</a></li>
      {{/blocks}}
    </ul>
  </navigation>
  <main class="RSG-content">
  {{#blocks}}
    <h1>{{title}}</h1>
    <strong>Descriptions:</strong> <br>
      <ul>
        {{#descriptions}}
          <p>{{.}}</p>
        {{/descriptions}}
      </ul>
    {{#.}}
    <article id="{{title}}">

      {{#sections}}

        <strong>Descriptions:</strong> <br>
        <ul>
          {{#descriptions}}
            <p>{{.}}</p>
          {{/descriptions}}
        </ul>

        <strong>Classes:</strong> <br>
        <ul>
          {{#classes}}
            <p>{{key}} - {{value}}</p>
          {{/classes}}
        </ul>

        <strong>Markups:</strong> <br>
        {{#parsedMarkups}}
          {{{.}}} <br>
        {{/parsedMarkups}}



      {{/sections}}

    </article>
    {{/.}}
    <hr>
  {{/blocks}}

  </main>
</body>
</html>
